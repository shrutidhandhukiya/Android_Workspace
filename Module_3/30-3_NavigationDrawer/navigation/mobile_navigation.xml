<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.drawerapplication.ui.Home.HomeFragments"
        android:label="Home"
        tools:layout="@layout/fragment_home_fragments" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.drawerapplication.ui.Home.MyProfileFragments"
        android:label="My Profile"
        tools:layout="My Profile " />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.drawerapplication.ui.Home.MyCartFragments"
        android:label="My Cart"
        tools:layout="@layout/fragment_my_cart_fragments" />

    <fragment
        android:id="@+id/nav_Transaction"
        android:name="com.example.drawerapplication.ui.Home.TransactionFragment"
        android:label="My Cart"
        tools:layout="@layout/fragment_transaction" />

    <fragment
        android:id="@+id/nav_MySubscription"
        android:name="com.example.drawerapplication.ui.Home.SubscriptionFragment"
        android:label="MySubscription"
        tools:layout="@layout/fragment_subscription" />

    <fragment
        android:id="@+id/nav_AboutUs"
        android:name="com.example.drawerapplication.ui.Home.AboutUsFragment"
        android:label="About Us"
        tools:layout="@layout/fragment_about_us" />

    <fragment
        android:id="@+id/nav_ContactUs"
        android:name="com.example.drawerapplication.ui.Home.ContactUsFragment"
        android:label="Contact Us"
        tools:layout="@layout/fragment_contact_us" />

    <fragment
        android:id="@+id/nav_Terms"
        android:name="com.example.drawerapplication.TermsFragment"
        android:label="Terms &amp;Condition  "
        tools:layout="@layout/fragment_terms" />

    <fragment
        android:id="@+id/nav_Privacy"
        android:name="com.example.drawerapplication.ui.Home.PrivacyFragment"
        android:label="Privacy and Policy"
        tools:layout="@layout/fragment_privacy" />

    <fragment
        android:id="@+id/nav_LogOut"
        android:name="com.example.drawerapplication.ui.Home.LogoutFragment"
        android:label="LogOUT"
        tools:layout="@layout/fragment_logout" />

</navigation>